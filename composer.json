{
    "name": "shammaa/laravel-slug",
    "description": "Professional Arabic-friendly slug generator for Laravel with automatic model integration",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Shadi Shammaa",
            "email": "shadi.shammaa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shammaa\\LaravelSlug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shammaa\\LaravelSlug\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shammaa\\LaravelSlug\\LaravelSlugServiceProvider"
            ],
            "aliases": {
                "Slug": "Shammaa\\LaravelSlug\\Facades\\Slug"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

